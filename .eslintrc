{
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    //// __Seed builder__ > Autogenerated rules, add custom rules at the end
    "eqeqeq": "off",
    "react/jsx-key": "warn",
    "react/no-children-prop": "off",
    "react/no-unknown-property": "off",
    "react/prop-types": "warn",
    "no-empty": "off",
    "no-unused-vars": "off",
    "no-lone-blocks": "off",
    "max-len": ["warn", { "code": 120 }],
    "jsx-a11y/anchor-is-valid": "off",
    "jest/valid-describe": "off",
    //// __End autogenerated__ Include extra rules after this line
  }
}