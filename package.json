{
  "name": "reactjs-reference",
  "version": "0.1.0",
  "scripts": {
    "build-css": "node-sass-chokidar src/views -o src/resources/css && node-sass-chokidar src/resources/styles -o src/resources/css && node-sass-chokidar src/components -o src/resources/css  && node-sass-chokidar src/seed/helpers -o src/resources/css/seed &&  node-sass-chokidar src/seed/examples/styles -o src/resources/css/seed/examples",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "watch-css": "npm run build-css && node-sass-chokidar src/views -o src/resources/css --watch --recursive && node-sass-chokidar src/resources/styles -o src/resources/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --watchAll=false --silent",
    "coverage": "react-scripts test --watchAll=false --silent --coverage",
    "deploy": "node ./bin/www",
    "eject": "react-scripts eject"
  },
  "private": true,
  "dependencies": {
    "body-parser": "1.19.0",
    "debug": "4.3.1",
    "express": "4.17.1",
    "log": "6.0.0",
    "morgan": "1.10.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@apollo/react-hooks": "4.0.0",
    "@material-ui/core": "4.11.3",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.1",
    "apollo-boost": "0.4.9",
    "apollo-cache-inmemory": "1.6.6",
    "classnames": "2.3.1",
    "cross-fetch": "3.1.4",
    "docdash": "1.2.0",
    "eslint": "7.23.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "formik": "2.2.6",
    "graphql": "15.5.0",
    "jquery": "3.6.0",
    "jsdoc": "3.6.6",
    "jshint": "2.12.0",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-fetch-hook": "1.8.5",
    "react-ga": "3.3.0",
    "react-load-script": "0.0.6",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not ie < 9",
    "not op_mini all"
  ]
}