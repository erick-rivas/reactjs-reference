{
  "name": "reactjs-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.2.2",
    "@material-ui/icons": "3.0.1",
    "cross-fetch": "2.2.2",
    "i": "0.3.6",
    "jquery": "3.3.1",
    "npm": "6.4.1",
    "npm-run-all": "4.1.3",
    "react": "16.5.2",
    "react-css-modules": "4.7.7",
    "react-dom": "16.5.2",
    "react-load-script": "0.0.6",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.5",
    "react-scripts-cssmodules": "1.1.50",
    "react-svg": "6.0.18",
    "reactour": "1.9.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "1.3.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}