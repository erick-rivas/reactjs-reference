{
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./public/docs",
    "tutorials": "./docs",
    "recurse":true
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
            "./bin/config/docs/static"
        ]
      }
    }
  },
  "docdash": {
    "scripts": ["./style.css"],
    "sort": false,
    "sectionOrder": [ 
      "Tutorials",         
      "Modules",
       "Classes",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Interfaces"
    ]
  }
}