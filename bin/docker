# Delete previous containers
docker-compose -f bin/config/docker/docker-compose.dev.yml down

# Build project
docker-compose -f bin/config/docker/docker-compose.dev.yml build

# Generate docs
docker-compose -f bin/config/docker/docker-compose.dev.yml run reactjs npm run-script build-docs

# Restart server
docker-compose -f bin/config/docker/docker-compose.dev.yml up